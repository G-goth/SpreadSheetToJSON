{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "strict": true,
        "no-trailing-whitespace": false,
        "one-line": true,
        "forin": false,
        "no-string-literal": false,
        "prefer-for-of": false,
        "max-classes-per-file": [
            true, 5
        ],
        "max-line-length": [
            true, 200
        ]
    },
    "rulesDirectory": []
}